@model IEnumerable<DishClassLibrary.temp.test.Get_Ingredients>

@{
    ViewBag.Title = "IngredientNameSearch";
}

<h2>IngredientNameSearch</h2>

<p>
    @Html.ActionLink("Create New", "SetIngredient", "Home")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Ingredient)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Info)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Ingredient)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Info)
            </td>
        </tr>
    }

</table>
@Html.ActionLink("Search pool", "Index", "Search", new { @class = "btn btn-default" })
